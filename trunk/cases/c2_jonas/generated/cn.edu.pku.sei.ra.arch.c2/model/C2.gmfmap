          xsi:type="gmftool:CreationTool"
          href="C2.gmftool#//@palette/@tools.0/@tools.3"/>
      <diagramNode
          href="C2.gmfgraph#Connector"/>
    </ownedChild>
  </nodes>
  <links>
    <tool
        xsi:type="gmftool:CreationTool"
        href="C2.gmftool#//@palette/@tools.0/@tools.2"/>
    <diagramLink
        href="C2.gmfgraph#ComponentAbove"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="C2.ecore#//Component/below"/>
  </links>
  <links>
    <tool
        xsi:type="gmftool:CreationTool"
        href="C2.gmftool#//@palette/@tools.0/@tools.1"/>
    <diagramLink
        href="C2.gmfgraph#ComponentAbove"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="C2.ecore#//Component/above"/>
  </links>
  <diagram>
    <diagramCanvas
        href="C2.gmfgraph#C2"/>
    <domainModel
        href="C2.ecore#/"/>
    <domainMetaElement
        href="C2.ecore#//Architecture"/>
    <palette
        href="C2.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
